  <!-- BASE_LINK (RED) -->
  <link name="base_link">
    <inertial>
      <mass value="25.0" />
      <origin xyz="0 0 0" />
      <inertia  ixx="1.391" ixy="0.004"  ixz="0.0"  iyy="6.853"  iyz="0.0"  izz="6.125" />
    </inertial>
    <visual>
       <origin xyz="0 0 0" rpy="0 0 0" />
       <geometry>
	 <mesh filename="package://meshes/hq_suspension/xl_chassis.dae"/>
	   </geometry>
    </visual>
    <collision>
       <origin xyz="0 0 0" rpy="0 0 0 " />
       <geometry>
         <mesh filename="package://meshes/hq_suspension/xl_chassis.dae"/>
       </geometry>
    </collision>     
  </link>

